<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="500.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="com.example.curriculumvitae.controllers.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label alignment="CENTER" prefHeight="17.0" prefWidth="305.0" text="Здравствуй, студент" textAlignment="CENTER">
         <font>
            <Font name="Arial Bold" size="23.0" />
         </font>
      </Label>
      <Label prefHeight="113.0" prefWidth="460.0" text="Сейчас тебе предстоит создать свое резюме, поэтому внимательно читай вопросы и отвечай на них честно ведь именно с этим резюме будет знакомиться твой будущий работодатель." textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#startButton" prefHeight="56.0" prefWidth="93.0" text="Начать" />
      <Label fx:id="errorLog" prefHeight="67.0" prefWidth="481.0" />
   </children>
</VBox>
